<%- include('../includes/header.ejs')%>
<%- include('../includes/navigation.ejs')%>

<div class="">
    <div class="text-center"></div>
    <table class="table table-striped">
        <tr>
            <th>ID</th>
            <th>ID_USUARIO</th>
            <th>NOMBRE</th>
            <th>TIPO</th>
            <th>CALIFICACION</th>
        </tr>
        <% for(var i = 0; i<datos.length; i++){ %>
            <tr>
                <%datos[i].respuesta = (datos[i].respuesta==0)? 'incorrecta' : 'correcta';%>
                <td><%=datos[i].id%></td>
                <td><%=datos[i].id_usuario%></td>
                <td><%=datos[i].nombre%></td>
                <td><%=datos[i].tipo%></td>
                <td><%=datos[i].calificacion%></td>
            </tr>
        <%}%>
    </table>
</div>
</body> 